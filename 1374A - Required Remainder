#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
    ll x, y, n;
    cin >> x >> y >> n;

    if (n - n % x + y <= n) 
    {
        cout << n - n % x + y << endl;
    } 
    else 
    {
        cout << n - n % x - (x - y) << endl;
    }
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    cin >> T;
    while (T--) 
    {
        solve();
    }
}
