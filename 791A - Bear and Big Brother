    #include <bits/stdc++.h>
    #define ll long long int
    #define nline '\n'
    using namespace std;
     
    void solve()
    {
        ll a, b, years = 0;
        cin >> a >> b;
     
        if (a > b)
            years = 0;
        else
        {
            while (a <= b)
            {
                a *= 3;
                b *= 2;
                years++;
            }
        }
     
        cout << years << nline;
    }
     
    int main()
    {
        ios_base::sync_with_stdio(false);
        cin.tie(NULL);
        ll T = 1;
        while (T--) 
        {
            solve();
        }
     
    }
