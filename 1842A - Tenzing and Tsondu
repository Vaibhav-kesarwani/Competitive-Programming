#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
    ll n, m;
    cin >> n >> m;

    ll sumA = 0, sumB = 0;

    for (int i = 0; i < n; i++) {
        ll temp;
        cin >> temp;
        sumA += temp;;
    }

    for (int i = 0; i < m; i++) {
        ll temp;
        cin >> temp;
        sumB += temp;
    }

    if (sumA > sumB)
        cout << "Tsondu\n";
    else if (sumA < sumB)
        cout << "Tenzing\n";
    else
        cout << "Draw\n";
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    cin >> T;
    while (T--) 
    {
        solve();
    }

}
