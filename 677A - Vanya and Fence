    #include <bits/stdc++.h>
    #define ll long long int
    #define nline '\n'
    using namespace std;
     
    void solve()
    {
        ll n, h;
        cin >> n >> h;
        ll maxWid = 0;
     
        for (int i = 0; i < n; i++)
        {
            ll t;
            cin >> t;
     
            if (t > h)
                maxWid += 2;
            else
                maxWid++;
        }
        cout << maxWid << nline;
    }
     
    int main()
    {
        ios_base::sync_with_stdio(false);
        cin.tie(NULL);
        ll T = 1;
        while (T--) 
        {
            solve();
        }
    }
