    #include <bits/stdc++.h>
    #define ll long long int
    #define nline '\n'
    using namespace std;
     
    void solve()
    {
        ll n;
        cin >> n;
     
        double sum = 0;
        for (int i = 0; i < n; i++)
        {
            ll t;
            cin >> t;
            sum += t;
        }
        double ans = sum / n;
        cout << fixed << setprecision(12) << ans;
    }
     
    int main()
    {
        ios_base::sync_with_stdio(false);
        cin.tie(NULL);
        ll T = 1;
        while (T--) 
        {
            solve();
        }
     
    }
