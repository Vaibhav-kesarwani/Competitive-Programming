#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
    ll arr[4];
    cin >> arr[0] >> arr[1] >> arr[2] >> arr[3];

    sort(arr, arr + 4);

    cout << (arr[3] - arr[0]) << " " << (arr[3] - arr[1]) << " " << (arr[3] - arr[2]) << nline;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    while (T--) 
    {
        solve();
    }

}
