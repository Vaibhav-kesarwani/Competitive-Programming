    #include <bits/stdc++.h>
    #define ll long long int
    #define nline '\n'
    using namespace std;
     
    void solve()
    {
        ll n;
        cin >> n;
     
        cout << 2 << nline;
        for (int i = 1; i <= n; i += 2)
            for (int j = i; j <= n; j *= 2)
                cout << j << " ";
        cout << nline;
    }
     
    int main()
    {
        ios_base::sync_with_stdio(false);
        cin.tie(NULL);
        ll T = 1;
        cin >> T;
        while (T--) 
        {
            solve();
        }
     
    }
