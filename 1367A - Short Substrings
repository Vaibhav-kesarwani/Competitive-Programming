#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
    string b;
    cin >> b;

    string a = b.substr(0, 2);

    for (int i = 3; i < b.size(); i += 2) {
        a += b[i];
    }

    cout << a << endl;
    
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    cin >> T;
    while (T--) 
    {
        solve();
    }

}
