    #include <bits/stdc++.h>
    #define ll long long int
    #define nline '\n'
    using namespace std;
     
    void solve()
    {
        ll a, b;
        cin >> a >> b;
     
        if (a % b == 0)
            cout << 0 << nline; 
        else
        {
            ll rem = a % b;
            cout << b - rem << nline;
        }
    }
     
    int main()
    {
        ios_base::sync_with_stdio(false);
        cin.tie(NULL);
        ll T = 1;
        cin >> T;
        while (T--) 
        {
            solve();
        }     
    }
