#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;
 
void solve()
{
    ll a, b, c, d;
    cin >> a >> b >> c >> d;
    ll res = 0;
 
    if (b > a)
        res++;
    if (c > a)
        res++;
    if (d > a)
        res++;
 
    cout << res << nline;
}
 
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    cin >> T;
    while (T--) 
    {
        solve();
    }
}
