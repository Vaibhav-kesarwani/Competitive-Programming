#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
    ll a, b, c;
    cin >> a >> b >> c;

    ll maximum = max(a, max(b, c));
    ll minimum = min(a, min(b, c));

    cout << maximum - minimum << nline;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    while (T--) 
    {
        solve();
    }

}
