#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
    ll n;
    cin >> n;

    if (n < 7 or n == 9) {
        cout << "NO\n";
        return;
    }

    cout << "YES\n";
    if (n % 3 == 0)
        cout << 1 << " " << 4 << " " << (n - 5) << nline;
    else
        cout << 1 << " " << 2 << " " << (n - 3) << nline;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    cin >> T;
    while (T--) 
    {
        solve();
    }

}
