#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
    ll n, k;
    cin >> n >> k;

    bool hasOne = false;

    for (int i = 0; i < n; i++)
    {   
        int temp;
        cin >> temp;

        if (temp == 1)
            hasOne = true;
    }

    if (hasOne)
        cout << "YES\n";
    else
        cout << "NO\n";
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    cin >> T;
    while (T--) 
    {
        solve();
    }
}
