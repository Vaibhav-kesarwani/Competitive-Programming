#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
 	ll arr[3];
 	cin >> arr[0] >> arr[1] >> arr[2];

 	sort(arr, arr + 3);
 	cout << arr[1] << nline;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    cin >> T;
    while (T--) 
    {
        solve();
    }

}
